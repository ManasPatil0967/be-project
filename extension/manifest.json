{
    "manifest_version": 3,
    "name": "Digital Identity Extension",
    "description": "Base Level Extension",
    "version": "1.0",
    "action": {
        "default_popup": "popup/index.html",
        "default_icon": "public/icon.png"
    },
    "permissions": [
        "nativeMessaging",
        "storage",
        "activeTab",
        "tabs",
        "contextMenus",
        "cookies",
        "webRequest",
        "scripting",
        "storage"
    ],
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "host_permissions": [
        "http://34.122.47.164:3000/*",
        "http://34.31.130.113:3000/*"
    ]
}

